    margin-left: 0;
    padding-top: 25px;
    padding-right: 25px;
    padding-bottom: 25px;
    padding-left: 25px;
    background-color: rgb(236, 148, 42);
    border-top-width: 0;
    border-right-width: 0;
    border-bottom-width: 0;
    border-left-width: 0;
}

.category-button:hover {
    background-color: rgb(195, 123, 36);
}

.category-button:active {
    background-color: rgb(155, 101, 36);
}

.content-button {
